<span id='contractor-login-form'>
  <%= form_for(:session, url: login_path) do |f| %>

    <%= f.label :email %>
    <%= f.text_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Contractor Login" %>
    <%= link_to "Create New Account", sign_up_path, class: "btn" %> 
  <% end %>

</span>
