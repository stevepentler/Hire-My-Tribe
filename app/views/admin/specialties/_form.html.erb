<%= form_for [:admin, @specialty] do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :url_name %>
<%= f.text_field :url_name %>
<br>
<%= f.label :description %>
<%= f.text_field :description %>
<br>
<%= f.label :image %>
<%= f.file_field :image, class: "file-field" %>
<br>
<%= f.submit %>
<% end %>
